{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/check-prediction/(.*)",
      "dest": "/api/check-prediction.js"
    },
    {
      "src": "/api/flux-transfer-main",
      "dest": "/api/flux-transfer-main.js"
    },
    {
      "src": "/api/flux-transfer",
      "dest": "/api/flux-transfer.js"
    }
  ]
}
